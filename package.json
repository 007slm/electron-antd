{
  "name": "electron-antd",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "pre-build": "tsc -p ./build",
    "ts-run": "ts-node -P ./build/tsconfig.json -r build/test.ts",
    "ts-run-dev": "ts-node -P ./build/tsconfig.json build/tasks/dev-server.ts",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development BUILD_ENV=dev && npm run ts-run-dev",
    "dev:dev": "cross-env NODE_ENV=development BUILD_ENV=dev babel-node build/task/dev-server.js",
    "build:dev": "cross-env NODE_ENV=production BUILD_ENV=dev babel-node build/task/build.js",
    "build:daily": "cross-env NODE_ENV=production BUILD_ENV=daily babel-node build/task/build.js",
    "build:gray": "cross-env NODE_ENV=production BUILD_ENV=gray babel-node build/task/build.js",
    "build:prod": "cross-env NODE_ENV=production BUILD_ENV=prod babel-node build/task/build.js",
    "merge-to": "bash build/task/merge-to.sh"
  },
  "dependencies": {
    "antd": "^3.26.5",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-do-expressions": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-function-bind": "^7.7.4",
    "@babel/plugin-proposal-function-sent": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-proposal-pipeline-operator": "^7.7.7",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.7.7",
    "@types/classnames": "^2.2.9",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/node": "^12.7.4",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.1",
    "@types/webpack-dev-server": "^3.9.0",
    "@types/webpack-env": "^1.14.1",
    "@types/webpackbar": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
